{
  "name": "acordeon",
  "version": "1.0.0",
  "description": "A dead simple animated collapse for JS",
  "module": "src/index.js",
  "scripts": {
    "start": "webpack --watch --progress --mode=development",
    "build": "NODE_ENV=production webpack -p",
    "serve": "serve ./",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js'",
    "lint:format": "prettier --write 'src/**/*.js'"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afk-mcz/acordeon.git"
  },
  "keywords": [
    "vanilla-js",
    "collapse"
  ],
  "author": "Mario Carballo Zama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afk-mcz/acordeon/issues"
  },
  "homepage": "https://github.com/afk-mcz/acordeon#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "clean-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.4",
    "serve": "^10.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
